{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "scripts": {
    "start": "gatsby develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf public/* && gatsby build && cp CNAME public/CNAME",
    "commit": "git add public && git commit -m 'deployment'",
    "submodule": "git subtree push --prefix public origin master",
    "deploy": "npm run build && npm run commit && npm run submodule && git push origin --all"
  },
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "front-matter": "^2.0.7",
    "gatsby": "^0.10.0",
    "highlight.js": "^9.3.0",
    "history": "^2.0.2",
    "js-yaml": "^3.6.0",
    "markdown-it": "^6.0.1",
    "markdown-it-abbr": "^1.0.3",
    "markdown-it-attrs": "^0.2.0",
    "markdown-it-deflist": "^2.0.1",
    "markdown-it-footnote": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "object-assign": "^4.1.0",
    "react-document-title": "^2.0.1",
    "react-headroom": "^1.8.2",
    "react-responsive-grid": "^0.3.3",
    "react-router": "^2.4.0",
    "toml-js": "0.0.8",
    "typography": "^0.7.0"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a"
}
